    "eslint-plugin-react-refresh": "^0.4.19",
    "fetch-mock": "^12.5.2",
    "globals": "^16.0.0",
    "jsdom": "^26.1.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5",
    "vitest": "^3.2.3"
