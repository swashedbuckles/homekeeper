{
  "name": "@homekeeper/server",
  "version": "1.0.0",
  "description": "Backend API",
  "main": "dist/index.js",
  "private": true,
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "build": "tsc",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest --coverage",
    "test:watch": "vitest --watch",
    "lint": "eslint .",
    "lint:watch": "eslint . --watch",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "seed": "ts-node seeds/index.ts",
    "seed:clear": "ts-node seeds/index.ts --clear",
    "seed:quiet": "ts-node seeds/index.ts --quiet",
    "seed:minimal": "ts-node seeds/seed-minimal.ts"
  },
  "dependencies": {
    "@homekeeper/shared": "*",
    "aws-sdk": "^2.1472.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^2.2.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.0",
    "express-validator": "^7.0.1",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.15.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "validator": "^13.15.0",
    "winston": "^3.10.0",
    "zod": "^3.25.28"
  },
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "@types/bcryptjs": "^2.4.4",
    "@types/body-parser": "^1.19.5",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.14",
    "@types/express": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.3",
    "@types/morgan": "^1.9.6",
    "@types/multer": "^1.4.8",
    "@types/node": "^20.8.2",
    "@types/passport": "^1.0.17",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/supertest": "^2.0.14",
    "@types/validator": "^13.15.1",
    "@typescript-eslint/eslint-plugin": "^8.34.0",
    "@typescript-eslint/parser": "^8.34.0",
    "@vitest/coverage-v8": "^3.1.4",
    "eslint": "^9.28.0",
    "eslint-import-resolver-typescript": "^4.4.3",
    "eslint-plugin-import": "^2.31.0",
    "faker-js": "^1.0.0",
    "mongodb-memory-server": "^10.1.4",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.4",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "typescript-eslint": "^8.34.0",
    "vitest": "^3.1.4"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
